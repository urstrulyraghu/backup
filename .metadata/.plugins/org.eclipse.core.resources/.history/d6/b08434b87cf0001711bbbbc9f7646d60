package com.accolite.message;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class Message {
	int empId;
	String name;
	
	public Message() {
		super();
	}
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public Message(int empId, String name) {
		super();
		this.empId = empId;
		this.name = name;
	}
	
}
